<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.asus.matchstick">

    <application
        android:allowBackup="true"
        android:icon="@drawable/last_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Splashscreen"
            android:label="@string/app_name"
            android:theme="@style/Theme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".option"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".Choice"
            android:theme="@style/AppTheme" />
        <activity android:name=".number1" />
        <activity android:name=".number2" />
        <activity android:name=".FirstTry" />
        <activity android:name=".Second_Try" />
        <activity android:name=".androidNum" />
        <activity android:name=".android" />
        <activity android:name=".punish"></activity>
    </application>

</manifest>